{% extends 'base.html.twig' %}

{% block title %}Locataire{% endblock %}

{% block body %}
    <h1>Locataire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ locataire.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ locataire.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ locataire.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ locataire.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ locataire.telephone }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ locataire.adresse }}</td>
            </tr>
            <tr>
                <th>Complement</th>
                <td>{{ locataire.complement }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ locataire.ville }}</td>
            </tr>
            <tr>
                <th>Code_postal</th>
                <td>{{ locataire.codePostal }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_locataire_index') }}">back to list</a>

    <a href="{{ path('app_locataire_edit', {'id': locataire.id}) }}">edit</a>

    {{ include('locataire/_delete_form.html.twig') }}
{% endblock %}
